// dijkstra 邻接矩阵方式
#include <bits/stdc++.h>
using namespace std;
const int maxn = 210;
const int INF = 0x3f3f3f;
int g[maxn][maxn], d[maxn];
bool vis[maxn];

int main(){
	int n;
	cin >> n;
	for(int i=1; i<=n; i++)
		for(int j=1; j<=n; j++)
			g[i][j] = INF;
	for(int i=1; i<n; i++)
		for(int j=i+1; j<=n; j++)
			cin >> g[i][j];
	for(int i=1; i<=n; i++){
		vis[i] = false;
		d[i] = INF;
	}
	d[1] = 0;
	for(int i=1; i<=n; i++){
		int u, minx = INF;
		for(int j=1; j<=n; j++){
			if(!vis[j] && d[j]<minx){
				minx = d[j];
				u = j;
			}
		}
		vis[u] = true;
		for(int j=1; j<=n; j++)
			d[j] = min(d[j], d[u]+g[u][j]);
	}
	cout << d[n];
	return 0;
}
